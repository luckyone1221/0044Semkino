mixin greenLine()
	+b.green-line.d-none.d-lg-block
		.container
			+e.UL.menu
				if block
					block
				else
					li: a(href='#') Что такое домовладение
					li: a(href='#') Расположение
					li: a(href='#') Кварталы
					li: a(href='#') Почему "Новое Сёмкино"?
					li: a(href='#') Документы
mixin smSocItem(imgName)
	.col-auto
		+e.item(href='#')
			img(loading="lazy" src=`img/@2x/${imgName}.png` alt="")

mixin buyBtnBl()
	+e.buy-btn-cont
		+e.buy-btn купить
		+e.btns-dd
			+e.buy-btn(href='#') купить
			+e.buy-btn--dd(href='#') Проекты
			+e.buy-btn--dd(href='#') Цены
			+e.buy-btn--dd(href='#') Спецпредложения